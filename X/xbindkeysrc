"exec urxvtcd"
  Mod4 + Return

"exec urxvtcd -e sh"
  Mod4 + Control + Return

"exec urxvtcd -e ksh"
  Mod4 + Shift + Return

"exec urxvtcd -e zsh"
  Mod4 + Alt + Return

"exec br"
  Mod4 + b

"exec dmenu_run"
  Mod4 + d

"exec xgoc"
  Mod4 + g

"exec gimp"
  Mod4 + i

"exec urxvtcd -e tm"
  Mod4 + m

"exec dmp"
  Mod4 + p

"exec i3lock --color=#000000 --image ~/.i3/lock.png --nofork"
  Mod4 + slash

"exec amixer -q sset Master toggle"
  XF86AudioMute

"exec amixer -q sset Master 5%+ unmute"
  XF86AudioRaiseVolume

"exec amixer -q sset Master 5%- unmute"
  XF86AudioLowerVolume

"exec urxvtcd -e bcq"
  XF86Calculator
